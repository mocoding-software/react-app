{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2020",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "declaration": true,
    "removeComments": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "baseUrl": ".",
    "jsx": "react",
    "lib": ["ES2020"]
  }
}
