{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "Debug Basic Config Example",
      "request": "launch",
      "program": "${workspaceRoot}/packages/react-app/src/cli/index.ts",
      "args": ["config", "-c", "examples/basic/.moapp.json"],
      "outFiles": ["${workspaceRoot}/packages/react-app/lib/**/*.js"],
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "preLaunchTask": "build"
    },
    {
      "type": "node",
      "name": "Debug Basic Build Example",
      "request": "launch",
      "program": "${workspaceRoot}/packages/react-app/src/cli/index.ts",
      "args": ["build", "-e", "examples/basic"],
      "outFiles": ["${workspaceRoot}/packages/react-app/lib/**/*.js"],
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "preLaunchTask": "build"
    },
    {
      "type": "node",
      "name": "Debug Basic Serve Example",
      "request": "launch",
      "program": "${workspaceRoot}/packages/react-app/src/cli/index.ts",
      "args": ["serve", "-e", "examples/basic"],
      "outFiles": ["${workspaceRoot}/packages/react-app/lib/**/*.js"],
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "preLaunchTask": "build"
    }
  ]
}
